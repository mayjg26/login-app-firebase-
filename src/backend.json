{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A list of objects, each representing an entity with an image and a name.",
    "type": "object",
    "patternProperties": {
      "^[a-zA-Z0-9]+$": {
        "type": "object",
        "properties": {
          "image": {
            "type": "string",
            "format": "uri",
            "description": "URL of the image."
          },
          "name": {
            "type": "string",
            "description": "Name of the entity."
          }
        },
        "required": ["image", "name"],
        "additionalProperties": false
      }
    },
    "additionalProperties": false
  }
  